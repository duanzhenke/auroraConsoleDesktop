{
  "name": "auroraconsoledesktop",
  "version": "1.0.0",
  "description": "桌面应用联系",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duanzhenke/auroraConsoleDesktop.git"
  },
  "author": "zhenke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duanzhenke/auroraConsoleDesktop/issues"
  },
  "homepage": "https://github.com/duanzhenke/auroraConsoleDesktop#readme",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.electron.electron-quick-start",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}